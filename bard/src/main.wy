data Map(
    foo: int32
)

trait Fooable(
    fooat() :: int32
)

make Map into Fooable {
    func fooat(self) :: int32 {
        return self.foo;
    }
}

func main() {
    println("Hello, World!");
    mut p := @Point(24, 4);
    println(p.x);
    println(p.y);
    println(p.area());
    println(p.perimeter());
    map := @Map(42);
    println(map.fooat());
}

