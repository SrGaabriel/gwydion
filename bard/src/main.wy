func main() {
    mut array := *["Gwydion", "Is", "The", "Best!"];
    mut list := @List(array, 4);

    for i in 0..list.size()-1 {
        printf("Next: ");
        println(array[i]);
    }

    println("================");

    list.push("Bard");
    list.push("Is");
    list.push("Cool");
    list.push("Too!");

    for i in 0..list.size-1 {
        printf("Next: ");
        println(array[i]);
    }
}
