data Point(
    x: int32,
    y: int32
)

trait Shape(
    area(self) :: int32;
    perimeter(self) :: int32;
)

func main() {
    println("Hello, World!");
    p := @Point(6, 4);
    x := p.area();
    println(x);
}

make Point into Shape {
    func area(self) :: int32 {
        return self.x;
    }
    func perimeter(self) :: int32 {
        return 0;
    }
}